    <system:String x:Key="resError">Error</system:String>

    <system:String x:Key="resSaveResult">Save result</system:String>
    <system:String x:Key="resSqlQueryResult">Query execution result</system:String>
    <system:String x:Key="resInfo">Information</system:String>
    <system:String x:Key="resResultSaved">Query result was saved into file "{0}"</system:String>
    <system:String x:Key="resUseTableName">Use Table name</system:String>
    <system:String x:Key="resHDR">Is Header</system:String>
    <system:String x:Key="resQueryCompleted">SQL Query execution was successfully completed</system:String>

    <system:String x:Key="resTextForSearch">Text for search</system:String>
    <system:String x:Key="resResetSearch">Reset</system:String>

</ResourceDictionary>